bindsym $mod+p exec pidgin
bindsym $mod+c exec chromium
bindsym $mod+v exec vlc
bindsym $mod+n exec urxvt -bg black -fg white -e /usr/bin/newsbeuter -r $HOME/.newsbeuter/urls
bindsym $mod+Shift+v exec VirtualBox
bindsym $mod+x exec xlock -mode matrix
bindsym $mod+b exec urxvt -bg black -fg white -vb -e weechat-curses
bindsym $mod+m exec urxvt -bg black -fg white -vb -e mutt
